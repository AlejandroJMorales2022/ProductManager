<div class="container">
    <div class="text-center">
        <h6 class="card-title mb-3">Cart-Id: {{cartId}}</h6>
    </div>
    <div class="row d-flex justify-content-evenly align-items-center">
        {{#each products}}
        <div class="card col-sm-4 col-11 mb-2" style="width: 16rem;">
            <img src="/uploads/{{thumbnails}}" class="card-img-top img_prod_cart" alt="{{product.thumbnails}}">
            <div class="card-body">
                <h6 class="card-title text-center title_field_product">{{title}}</h6>
                <div class="container fluid p-0 m-0">
                    <div class="row d-flex flex-column justify-content-start align-items-center">
                        <div class="col-12">
                            <span class="title_field_product">CÃ³digo:</span><span> {{code}}</span>
                        </div>
                        <div class="col-12">
                            <span class="title_field_product">Precio:</span><span> ${{price}}</span>
                        </div>
                        <div class="col-12">
                            <span class="title_field_product">Stock:</span><span>{{stock}}un.</span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="d-flex justify-content-center">
                <div class="col-sm-6 col-12 d-flex justify-content-center align-items-center">
                    <p class="title_field_product me-2">Cantidad </p><p class="productQty d-flex justify-content-center align-items-center"> {{quantity}}</p>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>